{
    "_id": "_design/dvr",
    "views": {
	"recordings": {
	    "map": "function (doc) {\n  if (doc.type === \"recording\")\n    emit(doc._id, doc);\n}"
	},
	"scheduled": {
	    "map": "function (doc) {\n  if (doc.type === \"scheduled\")\n    emit(doc._id, doc);\n}"
	},
	"capturing": {
	    "map": "function (doc) {\n  if (doc.type === \"capturing\")\n    emit(doc._id, doc);\n}"
	}
    },
    "language": "javascript"
}
